<!DOCTYPE html>
<html lang="en">
<head>
      {{>bodyLinks}}
</head>
<body>

     {{>navbar}}


   <!-- main-content -->
    <div class="container-fluid main_header">
        <div class="row">
            <div class="col-md-10 col-12 mx-auto">

                <div class="row">
                    <!--left side view -->
                    <div class="col-md-6 col-12 main_header_left">  
                         <p>
                             Welcome to Weather teller app
                         </p>
                         <h1>
                              Get the <span>Real-Time Weather</span> Info of your city
                         </h1>
                         <a href="/weather">
                            <button> 
                                 Check now
                            </button>
                         </a>
                    </div> 
        
        
                     <!--right side view -->
                     <div class="col-md-6 col-12 main_header_right">
                        <div id="carouselExampleControls" class="carousel slide" data-ride="carousel">
                            <div class="carousel-inner">
                              <div class="carousel-item active">
                                <img class=" w-100" src="data:image/png;base64," alt="First slide">
                              </div>
                              <div class="carousel-item">
                                <img class=" w-100" src="https://i.pinimg.com/originals/bf/dd/b6/bfddb6a0f917932260550ff91928396a.png" alt="Second slide">
                              </div>
                              <div class="carousel-item">
                                <img class=" w-100" src="https://www.transparentpng.com/thumb/weather-report/android-weather-icons-png-12.png" alt="Third slide">
                              </div>
                            </div>
                            <a class="carousel-control-prev" href="#carouselExampleControls" role="button" data-slide="prev">
                              <span class="carousel-control-prev-icon" aria-hidden="true"></span>
                              <span class="sr-only">Previous</span>
                            </a>
                            <a class="carousel-control-next" href="#carouselExampleControls" role="button" data-slide="next">
                              <span class="carousel-control-next-icon" aria-hidden="true"></span>
                              <span class="sr-only">Next</span>
                            </a>
                          </div>
                          
                     </div>
                </div>    

            </div> 

        </div>

    </div>



    {{>footer}}


</body>
</html>